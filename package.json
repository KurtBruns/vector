{
  "name": "@kurtbruns/vector",
  "version": "0.1.11",
  "description": "A library for creating interactive vector graphics on the web.",
  "author": "Kurt Bruns",
  "main": "dist/lib/index.js",
  "browser": "dist/lib/index.js",
  "scripts": {
    "build": "yarn webpack --config config/webpack.prod.js && yarn tsc",
    "build:dev": "yarn webpack --config ./config/webpack.common.js",
    "start": "webpack serve --config config/webpack.dev.js",
    "clean": "rm -rf dist/",
    "clean-install": "rm -rf node_modules/ && yarn clean && yarn",
    "prepublish": "yarn run build",
    "package": "tar --exclude=\"*.js.map\" -czvf ./dist/library.tar.gz ./dist/ ./css/library.css",
    "rollup": "rollup --config",
    "test": "jest",
    "test:update": "jest -u",
    "tsc": "tsc --declaration true --sourceMap false --skipLibCheck",
    "watch": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kurtbruns/vector"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "dependencies": {
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.20.2",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/jest": "^26.0.16",
    "babel-loader": "^9.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-prettier": "^4.2.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^2.7.2",
    "postcss-loader": "^7.0.2",
    "postcss-preset-env": "^7.8.3",
    "rollup": "^2.28.2",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.4",
    "ts-node": "^9.1.1",
    "tslib": "^2.0.3",
    "typescript": "^3.9.7",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  }
}
